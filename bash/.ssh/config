Host    别名
    HostName        主机名
    Port            22
    User            用户名
    IdentityFile    密钥文件的路径

Host test
    HostName        192.168.1.5
    User            root

Host staging
    HostName        54.250.119.36
    User            centos
    IdentityFile    ~/.ssh/centos.jp.pem

Host web
    HostName web.aws.jp
    User ec2-user

Host jupyter
    HostName jupyter.aws.jp
    User udacity
